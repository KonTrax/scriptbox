{
  "name": "scriptbox",
  "publisher": "cubicle6",
  "repository": {
    "type": "git",
    "url": "https://github.com/calebmpeterson/scriptbox.git"
  },
  "license": "ISC",
  "displayName": "scriptbox",
  "description": "Extend VS Code without the extension boilerplate",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "javascript",
    "multi-root ready"
  ],
  "activationEvents": [
    "onCommand:extension.sayHello",
    "onCommand:extension.runScript"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      },
      {
        "command": "extension.runScript",
        "title": "Run Script"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
